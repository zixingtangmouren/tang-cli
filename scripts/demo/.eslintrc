{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": [
			"error",
			"never"
		],
		"linebreak-style": "off",
		"react/prop-types": "off",
		"react/destructuring-assignment": "off",
		"no-param-reassign": "off",
		"import/prefer-default-export": "off",
		"import/no-named-as-default": "off",
		"react/button-has-type": "off"
	}
}